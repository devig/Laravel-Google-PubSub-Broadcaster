{
    "name": "devig/laravel-google-pubsub-broadcaster",
    "description": "Broadcast events to Google PubSub with Laravel",
    "type": "library",
    "require": {
        "php": "^7 || ^8",
        "google/cloud-pubsub": "^1.35",
        "illuminate/support": "^8 || ^9",
        "illuminate/broadcasting": "^8 || ^9"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Valentin Prugnaud",
            "email": "valentin@whatdafox.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "WhatDaFox\\GoogleBroadcaster\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "WhatDaFox\\GoogleBroadcaster\\GoogleBroadcasterServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
